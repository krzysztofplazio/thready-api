{
  "ConnectionStrings": {
    "ThreadyDatabase": "Server=127.0.0.1;Port=54321;Database=threadydatabase;Uid=threadyuser"
  },
  "JwtOptions": {
    "Issuer": "http://localhost:5007",
    "Audience": "http://localhost:5007",
    "Secret": "",
    "ExpirationSeconds": 5,
    "RefreshToken": {
      "ExpiryDays": 7
    }
  },
  "DefaultUsersRole": 1,
  "Serilog": {
    "Using": [ "Serilog.Exceptions", "Serilog", "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "System": "Information",
        "Microsoft": "Information",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341",
          "apiKey": "none",
          "outputTemplate": "({Timestamp:yyyy-MM-dd HH:mm:ss} [Level]: {Level}) {Message:lj} <s:{SourceContext}>{NewLine}{Exception}",
          "restrictedToMinimumLevel": "Verbose"
        }
      },
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console",
              "Args": {
                "outputTemplate": "({Timestamp:yyyy-MM-dd HH:mm:ss} [Level]: {Level}) {Message:lj} <s:{SourceContext}>{NewLine}{Exception}",
                "restrictedToMinimumLevel": "Information"
              }
            }
          ]
        }
      },
      {
        "Name": "Graylog",
        "Args": {
          "hostnameOrAddress": "172.31.183.150",
          "port": "12201",
          "transportType": "Udp"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithExceptionDetails" ],
    "Properties": {
      "Environment": "LocalDev"
    }
  },
  "AllowedHosts": "*"
}
